#!/usr/bin/python
#!encoding: UTF-8

import sys
              
decimalespi = "3.1415926535897931159979634685441852" #Declaración de la función
def aprox(n):
  if(n!=0):
    suma=0.0 
    for i in range(1,n+1):
      xi = ((i - 0.5)/float(n))
      f_xi = 4/(1+xi**2)
      suma = suma + f_xi
      a = float(i-1)/n
      b = float(i)/n
      #print 'Subintervalo: [%f, %f] xi: %f f_xi: %f ' % (a, b, xi, f_xi)
 
  pi = suma/n
  return pi
              
n = int(sys.argv[1]) 
m = int(sys.argv[2]) 
lista=[]
for j in range(1,m+1):
  aproxpi=aprox(j*m)
  lista.append(aproxpi)
print lista   
   
    
    

  
